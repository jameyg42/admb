{
  "name": "@metlife/admb-server",
  "version": "3.0.1",
  "description": "AppDynamics Metrics Browser Server/Services",
  "private": true,
  "author": "Jamey Graham <jgraham@metlife.com>",
  "license": "UNLICENSED",
  "main": "out/index.js",
  "scripts": {
    "clean": "rimraf ./out",
    "prebuild": "npm run clean",
    "build": "tsc",
    "watch": "tsc --watch",
    "pretest": "npm run build",
