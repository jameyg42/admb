    "deploy": "sh deploy.sh"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.17.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "esbuild": "^0.19.2",
    "node-ts": "^5.1.2",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.0",
    "typescript": "~4.8.2"
  }
}
