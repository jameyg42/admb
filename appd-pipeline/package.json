{
  "name": "@metlife/appd-pipeline",
  "version": "2.0.0",
  "description": "AppD metrics pipeline (grammar and runtime)",
  "private": true,
  "author": "Jamey Graham <jgraham@metlife.com>",
  "license": "UNLICENSED",
  "main": "out/index.js",
  "scripts": {
    "clean": "rimraf ./out",
    "prebuild": "npm run clean",
    "build:parser": "mkdirp out && lezer-generator src/pipeline.grammar --cjs --names -o out/_parser",
    "build": "npm run build:parser && tsc",
    "watch": "tsc --watch",
    "pretest": "npm run build",
    "test": "jest",
    "test:quick": "jest",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@lezer/lr": "^0.15.0"
  },
  "devDependencies": {
    "@lezer/generator": "^0.15.0",
    "jest": "^27.4.7",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  }
}
