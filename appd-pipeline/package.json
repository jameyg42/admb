{
  "name": "@metlife/appd-pipeline",
  "version": "3.1.0",
  "description": "AppD metrics pipeline (grammar and runtime)",
  "author": "Jamey Graham <jgraham@metlife.com>",
  "license": "UNLICENSED",
  "main": "out/index.js",
  "types": "out/index.d.ts",
  "files": [
    "/out"
  ],
  "scripts": {
    "clean": "rimraf ./out",
    "prebuild": "npm run clean",
    "build:parser": "mkdirp out/lang && lezer-generator src/lang/pipeline.grammar --cjs --names -o out/lang/pipeline.grammar",
    "build:ts": "tsc",
    "build:incremental": "tsc --incremental",
    "build": "npm run build:parser && npm run build:ts",
    "watch": "tsc --watch",
    "pretest": "npm run build",
    "test": "jest",
    "test:quick": "jest",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@lezer/common": "^1.0.1",
    "@lezer/lr": "^1.2.3",
    "@metlife/appd-libmetrics": "*",
    "@metlife/appd-libutils": "*",
    "@metlife/appd-services": "*",
    "parse-duration": "^1.0.2",
    "rxjs": "~7.5.0"
  },
  "devDependencies": {
    "@lezer/generator": "^1.1.1",
    "jest": "27.5.1",
    "mkdirp": "^2.1.0",
    "rimraf": "^4.1.0",
    "typescript": "~4.8.2"
  }
}
